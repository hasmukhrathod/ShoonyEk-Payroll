<h1>Listing form_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @form_categories.each do |form_category| %>
  <tr>
    <td><%= form_category.name %></td>
    <td><%= link_to 'Show', form_category %></td>
    <td><%= link_to 'Edit', edit_form_category_path(form_category) %></td>
    <td><%= link_to 'Destroy', form_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Form category', new_form_category_path %>
